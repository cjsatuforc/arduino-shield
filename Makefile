all:
	./drl-optimize.py arduino-shield.drl > arduino-shield.opt.drl
	# switched back and front grb files here!
	pcb2gcode --back=arduino-shield-F_Cu.gbr --drill=arduino-shield.opt.drl --front=arduino-shield-B_Cu.gbr --outline=arduino-shield-Cmts_User.gbr --output-dir=. --postamble=postamble.ngc --preamble=preamble.ngc --dpi=1000 --metric=true --metricoutput=true --mirror-absolute=false --optimise=true --zchange=30.0000 --zero-start=true --zsafe=1.0000 --extra-passes=0 --mill-feed=600 --mill-speed=10000 --offset=1.00 --zwork=-0.030 --drill-feed=1000 --drill-front=false --drill-speed=20000 --milldrill=false --nog81=false --onedrill=true --zdrill=-1.6500 --bridges=0.5000 --bridgesnum=2 --cut-feed=600 --cut-infeed=10.0000 --cut-speed=10000 --cutter-diameter=2.0000 --fill-outline=true --outline-width=1 --zbridges=-0.6000 --zcut=-1.6500 --al-back=true --al-front=false --al-probefeed=100 --al-x=15.0000 --al-y=15.0000 --software=LinuxCNC
